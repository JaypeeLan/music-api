{
  "compilerOptions": {
    "outDir": "./build",
    "target": "es2016",
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "src/types/express/index.d.ts"],
  "exclude": ["node_modules"]
}
